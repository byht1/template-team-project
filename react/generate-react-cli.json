{
  "usesTypeScript": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.jsx",
        "styled": "templates/component/TemplateName.styled.js",
        "index": "templates/component/index.js"
      },
      "path": "src/components/modules",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withStyled": true,
      "withIndex": true
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.jsx",
        "lazy": "templates/page/TemplateName.lazy.jsx",
        "index": "templates/page/index.js"
      },
      "path": "src/page",
      "withLazy": true,
      "withStory": false,
      "withStyle": false,
      "withTest": false,
      "withIndex": true
    }
  }
}
